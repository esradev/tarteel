{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tarteel",
  "version": "0.1.0",
  "identifier": "com.tarteel.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tarteel",
        "fullscreen": false,
        "resizable": true,
        "decorations": true,
        "width": 1280,
        "height": 720,
        "minWidth": 800,
        "minHeight": 600,
        "maxWidth": 1920,
        "maxHeight": 1080,
        "visible": true,
        "alwaysOnTop": false,
        "transparent": false,
        "maximizable": true,
        "minimizable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
